{
    "name": "gemini-project",
    "version": "1.0.0",
    "scripts": {
        "start:backend": "cd RnDAI && npm start",
        "start:frontend": "cd gemini-angular-client && ng serve",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "build": "npm install && cd RnDAI && npm install && cd ../gemini-angular-client && npm install && npm run build"
    },
    "devDependencies": {
        "concurrently": "^6.2.0"
    },
    "dependencies": {
        "node-fetch": "^3.3.2"
    }
}
